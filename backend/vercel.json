{
  "version": 2,

  "rewrites": [
    { "source": "/api/login", "destination": "/api/index.js" },
    { "source": "/api/users/:path*", "destination": "/api/routes/crud.js" },
    { "source": "/api/crud/:path*", "destination": "/api/index.js" }
  ]

,
  "routes": [
    { "src": "/", "dest": "/" },
    { "src": "/api", "dest": "/api" },
    { "src": "/api/users", "dest": "/api/users" }

  ],
  "builds": [{ "src": "./api/index.js", "use": "@vercel/node"}]
}

